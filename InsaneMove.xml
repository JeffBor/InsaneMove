<?xml version="1.0" standalone="yes"?>
<settings>
	<tenant>
		<adminURL>https://tenant-admin.sharepoint.com</adminURL>
		<adminUser>spadmin@tenant.onmicrosoft.com</adminUser>
		<workerUser>spworker{0}@tenant.onmicrosoft.com</workerUser>
		<suffix>tenant.onmicrosoft.com</suffix>
	</tenant>
	<maxWorker>1</maxWorker>
	<notify>
		<smtp>mailhub</smtp>
		<from>sharepoint-no-reply@company.com</from>
	</notify>
	<optionalLimitServers>SPWFE1,SPWFE2</optionalLimitServers>
	<optionalRunOddAsDifferentUser>SPSERVICE</optionalRunOddAsDifferentUser>
</settings>